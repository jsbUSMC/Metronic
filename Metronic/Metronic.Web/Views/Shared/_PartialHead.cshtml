<head>
    <meta charset="utf-8" />
    <title>Metronic | FlowState2 Prototype</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link rel="home" id="ApplicationPath" href="~/" />
    <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    @Html.Stylesheet("~/Content/font-open-sans.css")
    @Html.Stylesheet("~/Content/assets/global/plugins/font-awesome/css/font-awesome.min.css")
    @Html.Stylesheet("~/Content/assets/global/plugins/simple-line-icons/simple-line-icons.min.css")
    @Html.Stylesheet("~/Content/assets/global/plugins/bootstrap/css/bootstrap.min.css")
    @Html.Stylesheet("~/Content/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css")
    <!-- END GLOBAL MANDATORY STYLES -->

    <!-- BEGIN THEME GLOBAL STYLES -->
    @Html.Stylesheet("~/Content/assets/global/css/components-md.min.css")
    @Html.Stylesheet("~/Content/assets/global/css/plugins-md.min.css")
    <!-- END THEME GLOBAL STYLES -->

    <!-- BEGIN THEME LAYOUT STYLES -->
    @Html.Stylesheet("~/Content/assets/layouts/layout/css/layout.min.css")
    <link rel="stylesheet" href="@Url.Content("~/Content/assets/layouts/layout/css/themes/darkblue.min.css")" id="style_color" type="text/css" />
    @Html.Stylesheet("~/Content/assets/layouts/layout/css/custom.min.css")
    <!-- END THEME LAYOUT STYLES -->
    
    <!-- BEGIN CORE PLUGINS -->
    @Html.Script("~/Content/assets/global/plugins/jquery.min.js")
    @Html.Script("~/Content/assets/global/plugins/bootstrap/js/bootstrap.min.js")
    @Html.Script("~/Content/assets/global/plugins/js.cookie.min.js")
    @Html.Script("~/Content/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js")
    @Html.Script("~/Content/assets/global/plugins/jquery.blockui.min.js")
    @Html.Script("~/Content/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js")
    @Html.Script("~/Scripts/Site/Metronic.Web.js")
    <!-- END CORE PLUGINS -->

    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    @Html.Script("~/Content/assets/global/scripts/app.js")
    <!-- END THEME GLOBAL SCRIPTS -->

    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    @Html.Script("~/Content/assets/layouts/layout/scripts/layout.min.js")
    @Html.Script("~/Content/assets/layouts/layout/scripts/demo.min.js")
    @Html.Script("~/Content/assets/layouts/global/scripts/quick-sidebar.min.js")
    <!-- END THEME LAYOUT SCRIPTS -->
</head>
